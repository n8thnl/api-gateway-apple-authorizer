{
    "compilerOptions": {
      "target": "es2020",
      "strict": true,
      "lib": [
        "es2020",
        "dom"
      ],
      "preserveConstEnums": true,
      "noEmit": true,
      "sourceMap": true,
      "module":"es2015",
      "moduleResolution":"node",
      "esModuleInterop": true, 
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "types": ["jest"]
    },
    "exclude": ["node_modules"]
  }
  